<!DOCTYPE html>
<html lang="">
<head>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="theme-color" content="{{ theme.menu_background_color }}">
  {% if page.is_homepage %}
  <link rel="canonical" href="{{ site.url }}" />
  {% endif %}
  {% unless page.content_editable %}<script src="{% asset_pipeline theme_api/v2/turbolinks-2.5.3-format.js %}"></script>{% endunless %}
  {% format_header %}
  <link href='https://static0.format.com/static/theme_api/v1/hosted_fonts-1fa30b0b16e206b04ff76bb76c5399ad06635e59d9fc2a338e015a0554cce89e.css' media='screen' rel='stylesheet' type='text/css' />
</head>
<body class="{% format_body_class %}">
  {% content_header %}
  {{ content_for_layout }}
  {% format_footer %}
  <script src="{% asset_pipeline theme_api/vendor/jquery-1.11.2.min.js %}"></script>
  <script src="{% asset_pipeline theme_api/vendor/imagesLoaded-1.js %}"></script>
  <script src="{% asset_pipeline theme_api/vendor/masonry.pkgd-3.3.1.min.js %}"></script>
  <script src="{% asset js/vendor/jquery.fancybox.min.js %}"></script>
  <script src="{% asset js/vendor/reframe.min.js %}"></script>
  <script src="{% asset js/vendor/lazysizes.min.js %}" async></script>
  <script src="{% asset js/vendor/ls.parent-fit.min.js %}"></script>
  <script src="{% asset js/vendor/ls.respimg.min.js %}"></script>
  <script src="{% asset js/vendor/swiper.min.js %}"></script>
  <script src="{% asset js/theme.js %}"></script>
  <style>
    html body #using {
      bottom: 37px!important;
      right: 30px!important;
    }
  </style>
</body>
</html>
